ate({ nameError, regError, passError, confirmPassError });\r\n      return false;\r\n    }\r\n\r\n    return true;\r\n  };\r\n\r\n  validate2 = () => {\r\n    let emailError = \"\";\r\n    let phoneError = \"\";\r\n    let gitError = \"\";\r\n    var re = /^[a-zA-Z0-9.!#$%&'+=?^_`{|}~-]+@vitstudent.ac.in$/;\r\n\r\n    if (!this.state.email) {\r\n      emailError = \"Email field cannot be left empty\";\r\n    } else if (!re.test(this.state.email)) {\r\n      emailError = \"Enter a valid VIT Email ID\";\r\n    }\r\n\r\n    if (!this.state.phone) {\r\n      phoneError = \"Enter a valid Mobile Number\";\r\n    }\r\n\r\n    else if (this.state.phone.length!=10) {\r\n      phoneError = \"Mobile Number should be 10 digits long\";\r\n    }\r\n\r\n    if (this.state.yearofstudy === 2) {\r\n      if(!this.state.github) {\r\n        gitError = \"GitHub Link is mandatory for 2nd year students\";\r\n      }\r\n    }\r\n\r\n    if (emailError || phoneError || gitError) {\r\n      this.setState({ emailError, phoneError, gitError });\r\n      return false;\r\n    }\r\n\r\n    return true;\r\n  };\r\n\r\n  createAccountClickHandler = (event) => {\r\n    event.preventDefault();\r\n    this.validate();\r\n\r\n    if (this.validate()) {\r\n      this.setState({ firstPage: false });\r\n    }\r\n  };\r\n  inputChangeHandler = (e, s) => {\r\n    this.setState({ [s]: e.target.value });\r\n  };\r\n  formSubmitHandler = (e, a) => {\r\n    this.validate2();\r\n\r\n    console.log(this.state.yearofstudy);\r\n\r\n    if(this.validate2()) {\r\n      const data = JSON.stringify({\r\n        name: this.state.name,\r\n        regno: this.state.regno,\r\n        email: this.state.email,\r\n        phone: this.state.phone,\r\n        yearofstudy: this.state.yearofstudy,\r\n        password: this.state.password,\r\n        githubLink: this.state.github,\r\n      });\r\n      var config = {\r\n        method: \"post\",\r\n        url: \"https://adgrecruitments.herokuapp.com/user/signup\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        data: data,\r\n      };\r\n\r\n      axios(config)\r\n          .then(function (response) {\r\n            console.log(JSON.stringify(response.data));\r\n            a.history.push(\"/login\");\r\n          })\r\n          .catch(function (error) {\r\n            alert(error.response.data.message);\r\n            console.log(error.success);\r\n          });\r\n    }\r\n  };\r\n  componentDidMount() {\r\n    if (sessionStorage.getItem(\"Token\")) {\r\n      this.props.history.replace(\"/selection\");\r\n    }\r\n  }\r\n  eyeClickHandler = () => {\r\n    this.setState({ showPass: !this.state.showPass });\r\n  };\r\n  eyeClickHandlerC = () => {\r\n    this.setState({ showCPass: !this.state.showCPass });\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <Background>\r\n        <form autoComplete='false'>\r\n          {this.state.firstPage ? (\r\n            <div>\r\n              <div className='heading'>Sign Up</div>\r\n              <div className='input-grp'>\r\n                <label>Name</label>\r\n\r\n                <input\r\n                  className='input name-input'\r\n                  type='text'\r\n                  value={this.state.name}\r\n                  placeholder='Enter your name'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"name\");\r\n                  }}\r\n                />\r\n              </div>\r\n              {this.state.nameError ? (\r\n                <div className='error'>{this.state.nameError}</div>\r\n              ) : null}\r\n              <div className='input-grp '>\r\n                <label>Registration Number</label>\r\n                <input\r\n                  autoComplete='off'\r\n                  onFocus={this.onFocus}\r\n                  className='input t-uc'\r\n                  value={this.state.regno}\r\n                  type='text'\r\n                  placeholder='Enter Registration Number'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"regno\");\r\n                  }}\r\n                />\r\n              </div>\r\n              {this.state.regError ? (\r\n                <div className='error'>{this.state.regError}</div>\r\n              ) : null}\r\n              <div className='input-grp'>\r\n                <label>Password</label>\r\n                <input\r\n                  className='input'\r\n                  type={`${this.state.showPass ? \"text\" : \"password\"}`}\r\n                  style={{ marginBottom: 10, position: \"relative\" }}\r\n                  value={this.state.password}\r\n                  placeholder='Enter Your Password'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"password\");\r\n                  }}\r\n                />\r\n                <div\r\n                  className={`lgn-eye ${\r\n                    this.state.showPass ? \"lgn-eye-t\" : null\r\n                  }`}\r\n                  onClick={this.eyeClickHandler}>\r\n                  <i className={`fas fa-eye  `}></i>\r\n                </div>\r\n              </div>\r\n              {this.state.passError ? (\r\n                <div className='error'>{this.state.passError}</div>\r\n              ) : null}\r\n              <div className='input-grp'>\r\n                <label>Confirm Password</label>\r\n                <input\r\n                  className='input'\r\n                  type={`${this.state.showCPass ? \"text\" : \"password\"}`}\r\n                  style={{ marginBottom: 10, position: \"relative\" }}\r\n                  value={this.state.confirmPass}\r\n                  placeholder='Confirm Password'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"confirmPass\");\r\n                  }}\r\n                />\r\n                <div\r\n                  className={`lgn-eye ${\r\n                    this.state.showCPass ? \"lgn-eye-t\" : null\r\n                  }`}\r\n                  onClick={this.eyeClickHandlerC}>\r\n                  <i className={`fas fa-eye  `}></i>\r\n                </div>\r\n              </div>\r\n              {this.state.confirmPassError ? (\r\n                <div className='error'>{this.state.confirmPassError}</div>\r\n              ) : null}\r\n              <div className='sub-btn' onClick={this.createAccountClickHandler}>\r\n                Next\r\n              </div>\r\n            </div>\r\n          ) : (\r\n            <div>\r\n              <div className='heading'>Sign Up</div>\r\n              <div className='input-grp'>\r\n                <label>Phone Number</label>\r\n                <input\r\n                  className='input'\r\n                  type='text'\r\n                  value={this.state.phone}\r\n                  placeholder='Enter Your Phone Number'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"phone\");\r\n                  }}\r\n                />\r\n              </div>\r\n              {this.state.phoneError ? (\r\n                <div className='error'>{this.state.phoneError}</div>\r\n              ) : null}\r\n              <div className='input-grp'>\r\n                <label>VIT Email</label>\r\n                <input\r\n                  className='input'\r\n                  type='text'\r\n                  value={this.state.email}\r\n                  placeholder='Enter Your VIT Email'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"email\");\r\n                  }}\r\n                />\r\n              </div>\r\n              {this.state.emailError ? (\r\n                <div className='error'>{this.state.emailError}</div>\r\n              ) : null}\r\n              <div className='input-grp'>\r\n                <label>GitHub Link  (Mandatory for 2nd year students)</label>\r\n                <input\r\n                  className='input'\r\n                  type='text'\r\n                  value={this.state.github}\r\n                  placeholder='Enter Your GitHub Handle'\r\n                  onChange={(event) => {\r\n                    this.inputChangeHandler(event, \"github\");\r\n                  }}\r\n                />\r\n              </div>\r\n              {this.state.gitError ? (\r\n                <div className='error'>{this.state.gitError}</div>\r\n              ) : null}\r\n              <div\r\n                className='sub-btn'\r\n                onClick={(event) => {\r\n                  this.formSubmitHandler(event, this.props);\r\n                }}>\r\n                Sign Up\r\n              </div>\r\n            </div>\r\n          )}\r\n        </form>\r\n      </Background>\r\n    );\r\n  }\r\n}\r\n\r\nexport default SignUp;\r\n","D:\\ADG-Recruitment-Portal ----latest\\ADG-reqruitment-portal\\src\\components\\Forms\\Landing\\Landing.js",[],"D:\\ADG-Recruitment-Portal ----latest\\ADG-reqruitment-portal\\src\\components\\Forms\\SignUp\\Login\\Login.js",[],"D:\\ADG-Recruitment-Portal ----latest\\ADG-reqruitment-portal\\src\\components\\Forms\\SignUp\\ForgotPassword\\ForgotPassword.js",[],"D:\\ADG-Recruitment-Portal ----latest\\ADG-reqruitment-portal\\src\\components\\Quiz\\DesignQuiz.js",[],"D:\\ADG-Recruitment-Portal ----latest\\ADG-reqruitment-portal\\src\\components\\Forms\\Domain&Instructions\\Thankyou.jsx",["79"],"import React, { useState } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport classes from \"./styles.module.css\";\r\nimport Background from \"../../../hoc/Background/Background\";\r\n\r\nconst ThankYou = ()=>{\r\n    return(\r\n        <Background>\r\n            <div className={classes.wrapper}>\r\n            <div>Your responses have been saved!</div>\r\n            <p>You can attempt quiz from other domains if you haven't</p>\r\n       