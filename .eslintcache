 url: \"https://adgrecruitments.herokuapp.com/user/resendverification\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      data: data,\n    };\n\n    axios(config)\n      .then((response) => {\n        // console.log(JSON.stringify(response.data));\n      })\n      .catch((error) => {\n        alert(error?.response?.data?.message);\n        // console.log(error.success);\n      });\n  };\n\n  countdown = () => {\n    let cooldownTimer = setInterval(() => {\n      this.setState({ cooldown: this.state.cooldown - 1 });\n      if (this.state.cooldown <= 0) {\n        this.setState({ cooldown: 0 });\n        clearInterval(cooldownTimer);\n      }\n    }, 1000);\n  };\n\n  render() {\n    const loader = (\n      <div style={{ margin: \"150px auto\" }}>\n        <img src={adggif} height={100} alt=\"ADG gif loader\" />\n      </div>\n    );\n    if (this.state.loading) {\n      return <Background>{loader}</Background>;\n    }\n    return (\n      <>\n        <Background>\n          <form autoComplete=\"false\">\n            {!this.state.verificationPage ? (\n              this.state.firstPage ? (\n                <div>\n                  <div className=\"heading\">Sign Up</div>\n                  <div className=\"input-grp\">\n                    <label>Name</label>\n\n                    <input\n                      className=\"input name-input\"\n                      type=\"text\"\n                      value={this.state.name}\n                      placeholder=\"Enter your name\"\n                      onChange={(event) => {\n                        this.inputChangeHandler(event, \"name\");\n                      }}\n                    />\n                  </div>\n                  {this.state.nameError ? (\n                    <div className=\"error\">{this.state.nameError}</div>\n                  ) : null}\n                  <div className=\"input-grp \">\n                    <label>Registration Number</label>\n                   